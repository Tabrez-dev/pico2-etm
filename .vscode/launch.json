{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "RP2350 ETM Debug",
            "type": "cortex-debug",
            "executable": "${workspaceFolder}/Blinky_Pico2_dual_core_nosdk/Output/arm_baremetal_pico2_dual_core_nosdk.elf",
            "request": "launch",
            "servertype": "external",
            "gdbTarget": "localhost:3333",
            "svdFile": "${workspaceFolder}/RP2350.svd",
            "showDevDebugOutput": "vscode",
            "runToEntryPoint": "main",
            "postAttachCommands": [
                "monitor reset halt",
                "load"
            ],
            "cwd": "${workspaceFolder}/Blinky_Pico2_dual_core_nosdk"
        }
    ]
}

